<view class="box"><block wx:for="{{newsList}}" wx:for-item="item" wx:for-index="index" wx:key="courseId"><view class="title"><image src="{{item.courseImage}}" mode="scaleToFill" data-index="{{index}}" data-item="{{item}}" data-event-opts="{{[['tap',[['goSuperMarket',['$event']]]]]}}" bindtap="__e"></image><view class="box_bottom"><view class="fl"><view class="fl_top">{{item.courseTitle}}</view><view class="fl_bottom">{{item.courseTitle}}</view></view><view class="fr"><view class="sc" data-count="{{item.collectCount}}" data-id="{{item.courseId}}" disabled="disabled" data-event-opts="{{[['tap',[['addone',['$event']]]]]}}" bindtap="__e">♥</view>{{item.collectCount+'个收藏'}}</view></view></view></block></view>